#include<stdio.h>
int main()
{
    int integerVar;
    float floatVar;
    char charVar;

    integerVar=5 ;
    floatVar=3.141593;
    charVar='a';

    printf("The inetger value:%d\n",integerVar);
    printf("The floating point value:%f\n",floatVar);
    printf("The Character value:%c\n",charVar);
    return 0 ;
}
